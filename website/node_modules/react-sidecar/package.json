{
  "name": "react-sidecar",
  "version": "0.1.1",
  "description": "React wrapper for Gitter sidecar",
  "scripts": {
    "build": "rimraf ./dist-modules && babel ./src --out-dir ./dist-modules",
    "prepublish": "npm run build"
  },
  "files": [
    "dist-modules",
    "src",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/bebraw/react-sidecar.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "gitter",
    "sidecar"
  ],
  "author": "Juho Vepsalainen <bebraw@gmail.com> (http://github.com/bebraw)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bebraw/react-sidecar/issues"
  },
  "main": "dist-modules",
  "module": "src",
  "jsnext:main": "src",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "react": ">= 0.11.2 < 16.0.0"
  }
}
